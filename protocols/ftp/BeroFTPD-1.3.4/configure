  --disable-upload        disable upload keyword in ftpaccess"
ac_help="$ac_help
  --disable-overwrite     disable overwrite keyword in ftpaccess"
ac_help="$ac_help
  --disable-hostxs        disable allow/deny keywords"
ac_help="$ac_help
  --disable-logfail       disable logging of failed logins"
ac_help="$ac_help
  --disable-logtoomany    disable logging of too many users"
ac_help="$ac_help
  --disable-private       disallow use of private file
                          (for site group and site gpass)"
ac_help="$ac_help
  --disable-dnsretry      don't retry failed DNS lookups"
ac_help="$ac_help
  --enable-anononly       allow only anonymous access"
ac_help="$ac_help
  --enable-paranoid       disable all SITE commands"
ac_help="$ac_help
  --enable-chmod-limit    limit SITE CHMOD values to 0000-0777"
ac_help="$ac_help
  --enable-skey           enable S/Key support"
ac_help="$ac_help
  --enable-bsd-auth       use BSD/OS authentication"
